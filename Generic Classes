            a_List_float.Add(10.3f);
            a_List_float.Add(20.5f);
            a_List_float.Add(30.65f);

            foreach (float i in a_List_float)
            {
                Console.WriteLine(i);
                //print(i);


            }

            Console.WriteLine();
            Console.WriteLine();


            Console.ReadLine();


        }
